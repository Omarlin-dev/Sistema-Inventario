{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "bootstrap@5.1.3",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "js/bootstrap.min.js",
        "css/bootstrap.min.css"
      ]
    },
    {
      "library": "jquery-loading-overlay@2.1.7",
      "destination": "wwwroot/lib/jquery-loading-overlay/",
      "files": [
        "loadingoverlay.min.js"
      ]
    },
    {
      "library": "sweetalert@2.1.2",
      "destination": "wwwroot/lib/sweetalert/",
      "files": [
        "sweetalert.min.js"
      ]
    },
    {
      "library": "font-awesome@6.0.0",
      "destination": "wwwroot/lib/font-awesome/",
      "files": [
        "js/all.min.js"
      ]
    },
    {
      "library": "datatable@2.0.2",
      "destination": "wwwroot/lib/datatable/",
      "files": [
        "css/datatable.min.css"
      ]
    },
    {
      "library": "datatables@1.10.21",
      "destination": "wwwroot/lib/datatables/",
      "files": [
        "js/jquery.dataTables.min.js",
        "css/jquery.dataTables.min.css"
      ]
    }
  ]
}